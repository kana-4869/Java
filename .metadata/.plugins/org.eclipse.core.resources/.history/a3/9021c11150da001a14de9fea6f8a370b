/*
 * 面向对象的三大特征：封装，继承，多态。
 * 
 */

package Demo03;

public class Demo02Method {
	public static void main(String[] args) {
		int[]array={5,10,20,50,100};
		
		int max=getMax(array);
	}
	
	public static int getMax(int[]array) {
		
		int max=array[0];
		for(int i=1;i<array.length;i++) {
			if(array[i]>max){
				max=array[i];
			}
			
			
		}
		return max;
	}
}
