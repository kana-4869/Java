package Demo06;

import java.util.ArrayList;

public class ArrayListEach {

	public static void main(String[] args) {
		
		ArrayList<String> list=new ArrayList<>();
		
		list.add("a");
		list.add("b");
		list.add("c");
		System.out.println(list);
		
		//±éÀú¼¯ºÏ
        for(int i=0;i<list.size();i++) {
        	System.out.println(list.get(i));
		}
	}
}
